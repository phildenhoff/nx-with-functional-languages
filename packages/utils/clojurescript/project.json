{
  "name": "utils-clojurescript",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/utils/clojurescript/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/utils/clojurescript",
        "main": "packages/utils/clojurescript/src/index.ts",
        "tsConfig": "packages/utils/clojurescript/tsconfig.lib.json",
        "assets": ["packages/utils/clojurescript/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/utils/clojurescript/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/packages/utils/clojurescript"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/utils/clojurescript"
      }
    }
  },
  "tags": []
}
