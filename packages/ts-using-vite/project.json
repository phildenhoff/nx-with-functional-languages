{
  "name": "ts-using-vite",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ts-using-vite/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/ts-using-vite",
        "main": "packages/ts-using-vite/src/index.ts",
        "tsConfig": "packages/ts-using-vite/tsconfig.lib.json",
        "assets": [
          "packages/ts-using-vite/*.md"
        ],
        "generatePackageJson": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/ts-using-vite/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": [
        "coverage/packages/ts-using-vite"
      ],
      "dependsOn": [
        "^build"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/ts-using-vite"
      }
    }
  },
  "tags": []
}